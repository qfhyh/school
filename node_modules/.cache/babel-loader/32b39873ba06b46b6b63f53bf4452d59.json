v>\r\n            <div class=\"result warning\" v-if=\"item.applyState == 1\">等待院书记审批</div>\r\n            <div class=\"result warning\" v-if=\"item.applyState == 2\">等待学生处审批</div>\r\n            <div class=\"result success\" v-if=\"item.applyState == 3\">审批通过</div>\r\n            <div class=\"result danger\" v-if=\"item.applyState == 8\">审批拒绝</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <ul class=\"list\">\r\n        <li>\r\n          <span class=\"title\">性别</span>\r\n          <span class=\"text\">\r\n            {{$store.state.system.userInfo.xBM == '1' ? '男'\r\n            : $store.state.system.userInfo.xBM == '2' ? '女' : '未知'}}\r\n          </span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">出生日期</span>\r\n          <span class=\"text\">\r\n            {{$store.state.system.userInfo.cSRQ\r\n            ? $store.state.system.userInfo.cSRQ\r\n            : '暂无'}}\r\n          </span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">民族</span>\r\n          <span class=\"text\">\r\n            {{$store.state.system.userInfo.mZMTEXT\r\n            ? $store.state.system.userInfo.mZMTEXT\r\n            : '暂无'}}\r\n          </span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">学号</span>\r\n          <span class=\"text\">\r\n            {{$store.state.system.userInfo.rYH\r\n            ? $store.state.system.userInfo.rYH\r\n            : '暂无'}}\r\n          </span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">身份证号</span>\r\n          <span class=\"text\">\r\n            {{$store.state.system.userInfo.sFZJH\r\n            ? $store.state.system.userInfo.sFZJH\r\n            : '暂无'}}\r\n          </span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">联系电话</span>\r\n          <span class=\"text\">\r\n            {{$store.state.system.userInfo.phone\r\n            ? $store.state.system.userInfo.phone\r\n            : '暂无'}}\r\n          </span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">家庭住址</span>\r\n          <span class=\"text\">\r\n            {{$store.state.system.userInfo.address\r\n            ? $store.state.system.userInfo.address\r\n            : '暂无'}}\r\n          </span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">学院</span>\r\n          <span class=\"text\">{{item.deptName}}</span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">专业</span>\r\n          <span class=\"text\">{{item.professionName}}</span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">班级</span>\r\n          <span class=\"text\">{{item.className}}</span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">补办理由</span>\r\n          <span class=\"text\">{{item.cause}}</span>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"top\">\r\n      <div class=\"between\">\r\n        <ul class=\"state\">\r\n          <li>\r\n            <img :src=\"$store.state.system.sftpPath + item.temp1\"\r\n            :onerror=\"defaultImg\" alt=\"\">\r\n            <span></span>\r\n            <span class=\"gray\">{{item.userName}}</span>\r\n            <span class=\"gray\">发起申请</span>\r\n            <span>{{item.createTime}}</span>\r\n          </li>\r\n          <li v-for=\"(o,index) in applyOption\" :key=\"index\">\r\n            <img :src=\"$store.state.system.sftpPath + o.zp\"\r\n            :onerror=\"defaultImg\" alt=\"\">\r\n            <span></span>\r\n            <span class=\"gray\">{{o.username}}</span>\r\n            <span class=\"success\" v-if=\"o.state == '1'\">已同意</span>\r\n            <span class=\"danger\" v-if=\"o.state == '2'\">已拒绝</span>\r\n            <span>{{o.operatorTime}}</span>\r\n            <br>\r\n            <p class=\"describe\">{{o.option}}</p>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { replaceDetail } from '@/services/replace';\r\nimport backButton from '@/components/backButton/backButton.vue';\r\n\r\nexport default {\r\n  name: 'replaceDetail',\r\n  data() {\r\n    return {\r\n      defaultImg: `this.src=\"${require('@/assets/tx@2x.png')}\"`,\r\n      item: {},\r\n      applyOption: [],\r\n      appJSObject: window.AppJSObject,\r\n      navback: false,\r\n    };\r\n  },\r\n  components: {\r\n    'back-button': backButton,\r\n  },\r\n  methods: {\r\n    onClickLeft() {\r\n      this.$router.go(-1);\r\n    },\r\n    async getDetail() {\r\n      if (!this.$route.params.item) {\r\n        this.navback = true;\r\n        const { data } = await replaceDetail({ id: this.appJSObject.getDetailId() });\r\n        this.item = data;\r\n        if (this.item.applyOption) {\r\n          this.applyOption = JSON.parse(this.item.applyOption);\r\n        }\r\n      } else {\r\n        const { data } = await replaceDetail({ id: this.$route.params.id });\r\n        this.item = data;\r\n        if (this.item.applyOption) {\r\n          this.applyOption = JSON.parse(this.item.applyOption);\r\n        }\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getDetail();\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.replaceDetail{\r\n  padding:46px 0 60px;\r\n  .success {\r\n    color: #51bf61 !important;\r\n  }\r\n  .warning{\r\n    color: #ea9542 !important;\r\n  }\r\n  .danger{\r\n    color: #ff5757 !important;\r\n  }\r\n  .between{\r\n    margin:0 12px;\r\n  }\r\n  .top{\r\n    margin-top: 11px;\r\n    background: #fff;\r\n  }\r\n  .personal-info{\r\n    padding:15px 0px;\r\n    border-bottom:1px solid #e7e7e7;\r\n  }\r\n  .avatar{\r\n    width: 48px;\r\n    height: 48px;\r\n    border-radius: 50%;\r\n    overflow: hidden;\r\n    margin-right: 4.266vw;\r\n  }\r\n  .name{\r\n    font-size: 16px;\r\n    color: rgb(51, 51, 51);\r\n    line-height: 6.4vw;\r\n  }\r\n  .result{\r\n    font-size: 14px;\r\n    line-height: 5.3333vw;\r\n  }\r\n  .list{\r\n    padding: 12px;\r\n    li{\r\n      line-height: 30px;\r\n      color:#333;\r\n      overflow: hidden;\r\n      .title{\r\n        display: block;\r\n        float: left;\r\n        width:65px;\r\n        text-align: right;\r\n        color:#999;\r\n        margin-right: 14px;\r\n      }\r\n      .text{\r\n        display: block;\r\n        float: left;\r\n      }\r\n    }\r\n  }\r\n  .state {\r\n    padding: 15px 0;\r\n    li {\r\n      line-height: 48px;\r\n      font-size: 15px;\r\n      height: 76px;\r\n      overflow: hidden;\r\n      position: relative;\r\n      img {\r\n        width:48px;\r\n        height: 48px;\r\n        border-radius: 50%;\r\n        margin: 0 15px 0 4px;\r\n        float: left;\r\n        position: relative;\r\n        z-index: 999999;\r\n      }\r\n      span {\r\n        display: block;\r\n        float: left;\r\n      }\r\n      .gray{\r\n        color:#666;\r\n      }\r\n      .describe {\r\n        font-size: 15px;\r\n        line-height: 20px;\r\n        margin-left: 67px;\r\n        color:#333;\r\n      }\r\n    }\r\n    li span:nth-child(2) {\r\n      width: 2px;\r\n      height: 100%;\r\n      border-left: 1px dashed #c3c3c3;\r\n      position: absolute;\r\n      left: 26px;\r\n      top: 2px;\r\n    }\r\n    li span:nth-child(3) {\r\n      width: 60px;\r\n      overflow: hidden;\r\n      display: block;\r\n      text-overflow: ellipsis;\r\n      white-space: nowrap;\r\n    }\r\n    li span:nth-child(4) {\r\n      margin: 0 0 0 3.74vw;\r\n    }\r\n    li span:nth-child(5) {\r\n      margin: 0 3.74vw 0 0;\r\n      float: right;\r\n      color: #999999;\r\n      font-size: 1.95vh;\r\n    }\r\n  }\r\n  li:last-child span:nth-child(2) {\r\n    border: none;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src\\views\\application\\replace"}]}