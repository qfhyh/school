,\r\n        { itemCode: '6', name: '复查通过' },\r\n        { itemCode: '7', name: '待复查' },\r\n      ],\r\n      classID: {\r\n        classId: this.$route.params.id,\r\n        checkStatus: '',\r\n      },\r\n      list: [],\r\n    };\r\n  },\r\n  methods: {\r\n    onClickLeft() {\r\n      this.$router.go(-1);\r\n    },\r\n    onClickRight(value) {\r\n      this.showState = true;\r\n    },\r\n    async getMessage(classId) {\r\n      const { data } = await studentList(classId);\r\n      this.list = data;\r\n    },\r\n    onState(value) {\r\n      this.showState = false;\r\n      this.state = value;\r\n      this.classID = {\r\n        classId: this.$route.params.id,\r\n        checkStatus: this.state.itemCode,\r\n      };\r\n      this.getMessage(this.classID);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.classID7 = {\r\n      classId: this.$route.params.id,\r\n      checkStatus: '7',\r\n    };\r\n    this.getMessage(this.classID7);\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n  .chooseState {\r\n    line-height: 46px;\r\n    position: absolute;\r\n    top:0;\r\n    right:20px;\r\n    z-index: 99;\r\n    font-size: 14px;\r\n    color:#1890ff;\r\n    .arrow{\r\n      font-size: 12px;\r\n      transform: rotate(90deg);\r\n    }\r\n  }\r\n  .classList{\r\n    padding-top: 46px;\r\n  }\r\n  .danger{\r\n    color: #ff5757 !important;\r\n  }\r\n  .between{\r\n    margin:0 12px;\r\n  }\r\n  .top{\r\n    margin-top: 11px;\r\n    background: #fff;\r\n  }\r\n  .item.head{\r\n    color: #323232;\r\n    border-top:1px solid #e7e7e7;\r\n  }\r\n  .item{\r\n    border-bottom:1px solid #e7e7e7;\r\n    overflow: hidden;\r\n    div{\r\n      width:33.33%;\r\n      float: left;\r\n      line-height: 44px;\r\n      text-align: center;\r\n    }\r\n  }\r\n  .item a{\r\n    width: 100%;\r\n    display: inline;\r\n  }\r\n</style>\r\n\r\n"]}]}