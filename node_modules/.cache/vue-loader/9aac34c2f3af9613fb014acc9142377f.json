;\r\n      .title{\r\n        display: block;\r\n        float: left;\r\n        width:65px;\r\n        text-align: right;\r\n        color:#999;\r\n        margin-right: 14px;\r\n      }\r\n      .text{\r\n        display: block;\r\n        float: left;\r\n      }\r\n    }\r\n  }\r\n  .state {\r\n    padding: 15px 0;\r\n    li {\r\n      line-height: 48px;\r\n      font-size: 15px;\r\n      height: 76px;\r\n      overflow: hidden;\r\n      position: relative;\r\n      img {\r\n        width:48px;\r\n        height: 48px;\r\n        border-radius: 50%;\r\n        margin: 0 15px 0 4px;\r\n        float: left;\r\n        position: relative;\r\n        z-index: 999999;\r\n      }\r\n      span {\r\n        display: block;\r\n        float: left;\r\n      }\r\n      .gray{\r\n        color:#666;\r\n      }\r\n      .describe {\r\n        font-size: 15px;\r\n        line-height: 20px;\r\n        margin-left: 67px;\r\n        color:#333;\r\n      }\r\n    }\r\n    li span:nth-child(2) {\r\n      width: 2px;\r\n      height: 100%;\r\n      border-left: 1px dashed #c3c3c3;\r\n      position: absolute;\r\n      left: 26px;\r\n      top: 2px;\r\n    }\r\n    li span:nth-child(3) {\r\n      width: 60px;\r\n      overflow: hidden;\r\n      display: block;\r\n      text-overflow: ellipsis;\r\n      white-space: nowrap;\r\n    }\r\n    li span:nth-child(4) {\r\n      margin: 0 0 0 3.74vw;\r\n    }\r\n    li span:nth-child(5) {\r\n      margin: 0 3.74vw 0 0;\r\n      float: right;\r\n      color: #999999;\r\n      font-size: 1.95vh;\r\n    }\r\n  }\r\n  li:last-child span:nth-child(2) {\r\n    border: none;\r\n  }\r\n}\r\n</style>\r\n"]}]}