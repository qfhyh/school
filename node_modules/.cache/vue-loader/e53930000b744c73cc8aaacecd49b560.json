: #333;\r\n     font-size: 16px;\r\n     padding: 15px 15px 0 15px;\r\n     position: relative;\r\n     &::before{\r\n      content: '*';\r\n      position: absolute;\r\n      left: 7px;\r\n      top: 20px;\r\n      font-size: 14px;\r\n      color: #f44;\r\n     }\r\n  }\r\n  textarea{\r\n    border:0;\r\n    width:100%;\r\n    height:100px;\r\n    padding:12px 15px;\r\n    resize: none;\r\n    font-size: 14px;\r\n    color:#999;\r\n  }\r\n}\r\n</style>\r\n"]}]}