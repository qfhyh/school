div class=\"result\">结束时间：{{item.endTime}}</div>\r\n            <div class=\"result warning\" v-if=\"item.offState === 0\">待审核</div>\r\n            <div class=\"result success\" v-else-if=\"item.offState === 1\">已通过</div>\r\n            <div class=\"result danger\" v-else>未通过</div>\r\n            <div class=\"cancel\">\r\n              <span :class=\"[btn, item.delLeave ? success : danger]\"\r\n              @click=\"goEvaluate()\">\r\n                {{item.delLeave ? '已销假' : '未销假'}}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div v-if=\"leaveData.length === 0\">\r\n        <no-data></no-data>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport backButton from '@/components/backButton/backButton.vue';\r\nimport noData from '@/components/noData/noData.vue';\r\nimport { getLeaveList } from '@/services/leave';\r\n\r\nexport default {\r\n  name: 'leaveBacklog',\r\n  components: {\r\n    backButton,\r\n    noData,\r\n  },\r\n  data() {\r\n    return {\r\n      leaveData: [],\r\n      btn: 'btn',\r\n      success: 'success',\r\n      danger: 'danger',\r\n    };\r\n  },\r\n  methods: {\r\n    onClickLeft() {\r\n      this.$router.go(-1);\r\n    },\r\n    goto(item) {\r\n      this.$router.push({\r\n        name: 'leaveApproval',\r\n        params: {\r\n          id: item.id,\r\n          item,\r\n        },\r\n      });\r\n    },\r\n    async getLeaveList() {\r\n      const { data } = await getLeaveList();\r\n      this.leaveData = data;\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getLeaveList();\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.leaveBacklog {\r\n  padding-top: 46px;\r\n  padding-bottom: 50px;\r\n  .success {\r\n    color: #51bf61 !important;\r\n  }\r\n  .warning{\r\n    color: #ea9542 !important;\r\n  }\r\n  .danger{\r\n    color: #ff5757 !important;\r\n  }\r\n  .between{\r\n    margin:0 12px;\r\n  }\r\n  .top{\r\n    margin-top: 11px;\r\n    background: #fff;\r\n  }\r\n  .personal-info{\r\n    padding:20px 0px 0px;\r\n    overflow: hidden;\r\n    position: relative;\r\n    &:last-child{\r\n      .info{\r\n        border: 0;\r\n      }\r\n    }\r\n    .avatar{\r\n      width: 48px;\r\n      height: 48px;\r\n      border-radius: 50%;\r\n      overflow: hidden;\r\n      margin-right: 14px;\r\n      float: left;\r\n    }\r\n    .info{\r\n      width:calc(100% - 62px);\r\n      float: left;\r\n      border-bottom:1px solid #e7e7e7;\r\n      .name{\r\n        font-size: 16px;\r\n        color: #333;\r\n        margin-bottom: 6px;\r\n      }\r\n      .result{\r\n        font-size: 15px;\r\n        color:#999;\r\n        line-height: 25px;\r\n      }\r\n    }\r\n    .con{\r\n      padding-bottom: 20px;\r\n    }\r\n    .date{\r\n      font-size: 14px;\r\n      color:#999;\r\n      float: right;\r\n      line-height: 24px;\r\n    }\r\n    .cancel {\r\n      position: absolute;\r\n      right: 0;\r\n      bottom: 24px;\r\n      font-size: 15px;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}