  <div>\r\n            <div class=\"name\">{{leaveData.userName}}</div>\r\n            <div class=\"result success\" v-if=\"leaveData.offState === 0\">待审核</div>\r\n            <div class=\"result success\" v-else-if=\"leaveData.offState === 1\">已通过</div>\r\n            <div class=\"result warning\" v-else-if=\"leaveData.offState === 3\">审核中</div>\r\n            <div class=\"result success\" v-else>未通过</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <ul class=\"list\">\r\n        <li>\r\n          <span class=\"title\">所在院系</span>\r\n          <span class=\"text\">{{leaveData.professionName}}</span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">所在班级</span>\r\n          <span class=\"text\">{{leaveData.className}}</span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">请假类型</span>\r\n          <span class=\"text\">{{leaveData.leaveTypeText}}</span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">开始时间</span>\r\n          <span class=\"text\">{{leaveData.startTime}}</span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">结束时间</span>\r\n          <span class=\"text\">{{leaveData.endTime}}</span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">请假时长</span>\r\n          <span class=\"text\">{{leaveData.leaveDay}}</span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">请假理由</span>\r\n          <span class=\"text\">{{leaveData.leaveReason}}</span>\r\n        </li>\r\n        <li>\r\n          <span class=\"title\">图片</span>\r\n          <div class=\"img\">\r\n            <img v-if=\"leaveData.photo\" :src=\"$store.state.system.sftpPath + leaveData.photo\">\r\n            <span v-if=\"!leaveData.photo\">暂无图片</span>\r\n          </div>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"top\">\r\n      <div class=\"between\">\r\n        <ul class=\"state\">\r\n          <li v-for=\"(item, index) in leaveFlow\" :key=\"index\">\r\n            <img :src=\"$store.state.system.sftpPath + item.zp\"\r\n            :onerror=\"defaultImg\" alt=\"\">\r\n            <span></span>\r\n            <span class=\"gray\">{{item.userName}}</span>\r\n            <span class=\"gray\" v-if=\"!item.status\">{{item.operator}}</span>\r\n            <span class=\"success\" v-if=\"item.status === '已通过'\">已通过</span>\r\n            <span class=\"danger\" v-if=\"item.status === '未通过'\">未通过</span>\r\n            <span class=\"warning\" v-if=\"item.status === '审核中'\">审核中</span>\r\n            <span>\r\n              {{item.operatorTime}}\r\n            </span>\r\n            <br>\r\n            <p class=\"describe\">{{item.option}}</p>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getLeaveDetail } from '@/services/leave';\r\nimport backButton from '@/components/backButton/backButton.vue';\r\n\r\nexport default {\r\n  name: 'leaveDetail',\r\n  data() {\r\n    return {\r\n      defaultImg: `this.src=\"${require('@/assets/tx@2x.png')}\"`,\r\n      leaveData: [],\r\n      leaveFlow: [],\r\n      appJSObject: window.AppJSObject,\r\n      navback: false,\r\n    };\r\n  },\r\n  components: {\r\n    'back-button': backButton,\r\n  },\r\n  methods: {\r\n    onClickLeft() {\r\n      this.$router.go(-1);\r\n    },\r\n    async getDetail() {\r\n      if (!this.$route.params.item) {\r\n        this.navback = true;\r\n        const { data } = await getLeaveDetail({ id: this.appJSObject.getDetailId() });\r\n        this.leaveData = data;\r\n        if (this.leaveData.applyOption) {\r\n          this.leaveFlow = JSON.parse(this.leaveData.applyOption);\r\n        }\r\n      } else {\r\n        const { data } = await getLeaveDetail({ id: this.$route.params.id });\r\n        this.leaveData = data;\r\n        if (this.leaveData.applyOption) {\r\n          this.leaveFlow = JSON.parse(this.leaveData.applyOption);\r\n        }\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getDetail();\r\n    // this.leaveData = this.$route.params.item;\r\n    // this.leaveFlow = JSON.parse(this.leaveData.applyOption);\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.leaveDetail {\r\n  padding-top: 46px;\r\n  padding-bottom: 50px;\r\n  .success {\r\n    color: #51bf61 !important;\r\n  }\r\n  .warning{\r\n    color: #ea9542 !important;\r\n  }\r\n  .danger{\r\n    color: #ff5757 !important;\r\n  }\r\n  .between{\r\n    margin:0 12px;\r\n  }\r\n  .top{\r\n    margin-top: 11px;\r\n    background: #fff;\r\n  }\r\n  .personal-info{\r\n    padding:15px 0px;\r\n    border-bottom:1px solid #e7e7e7;\r\n    .avatar{\r\n      width: 48px;\r\n      height: 48px;\r\n      border-radius: 50%;\r\n      overflow: hidden;\r\n      margin-right: 4.266vw;\r\n    }\r\n    .name{\r\n      font-size: 16px;\r\n      color: rgb(51, 51, 51);\r\n      line-height: 6.4vw;\r\n    }\r\n    .result{\r\n      font-size: 14px;\r\n      line-height: 5.3333vw;\r\n    }\r\n  }\r\n  .list{\r\n    padding: 12px;\r\n    li{\r\n      line-height: 30px;\r\n      color:#333;\r\n      .title{\r\n        display: inline-block;\r\n        width:66px;\r\n        color:#999;\r\n        margin-right: 14px;\r\n        text-align: right;\r\n      }\r\n      .img {\r\n        display: inline-block;\r\n      }\r\n    }\r\n  }\r\n  .state {\r\n    padding-top: 15px;\r\n    li {\r\n      line-height: 48px;\r\n      font-size: 15px;\r\n      height: 76px;\r\n      overflow: hidden;\r\n      position: relative;\r\n      img {\r\n        width:48px;\r\n        height: 48px;\r\n        border-radius: 50%;\r\n        margin: 0 15px 0 4px;\r\n        float: left;\r\n        position: relative;\r\n        z-index: 999999;\r\n      }\r\n      span {\r\n        display: inline-block;\r\n      }\r\n      .gray{\r\n        color:#666;\r\n\r\n      }\r\n      .describe {\r\n        font-size: 15px;\r\n        line-height: 20px;\r\n        margin-left: 67px;\r\n        color:#999;\r\n      }\r\n    }\r\n    li span:nth-child(2) {\r\n      width: 2px;\r\n      height: 100%;\r\n      border-left: 1px dashed #c3c3c3;\r\n      position: absolute;\r\n      left: 26px;\r\n      top: 2px;\r\n    }\r\n    li span:nth-child(3) {\r\n      width: 60px;\r\n      display: inline-block;\r\n      text-overflow: ellipsis;\r\n      white-space: nowrap;\r\n    }\r\n    li span:nth-child(4) {\r\n      margin: 0 0 0 3.74vw;\r\n    }\r\n    li span:nth-child(5) {\r\n      margin: 0 3.74vw 0 0;\r\n      float: right;\r\n      color: #999999;\r\n      font-size: 1.95vh;\r\n    }\r\n  }\r\n  li:last-child span:nth-child(2) {\r\n    border: none;\r\n  }\r\n  .detail{\r\n    padding: 15px 0;\r\n    border-bottom:1px solid #e7e7e7;\r\n  }\r\n  .btn-group{\r\n    padding: 0 38px;\r\n    margin: 20px 0;\r\n    .btn{\r\n        font-size: 17px;\r\n        margin: 0 auto 15px;\r\n        border-radius: 30px;\r\n        height: 6.8vh;\r\n        line-height: 6.8vh;\r\n        background-color: #FFFFFF;\r\n        color: #1890ff;\r\n        border: 1px solid #1890FF;\r\n    }\r\n    .blue{\r\n        background-color: #1890ff;\r\n        color: #FFFFFF;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}